<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jbuild4d.base.dbaccess.dao.DevDemoGenListMapper">
    <delete id="deleteAll">
        delete from TB4D_DEV_DEMO_GEN_LIST
    </delete>
    <select id="selectAll" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List" />
        from TB4D_DEV_DEMO_GEN_LIST ORDER by DDGL_ORDER_NUM DESC
    </select>
    <select id="nextOrderNum" resultType="integer">
        select case when max(DDGL_ORDER_NUM) is null then 1 else max(DDGL_ORDER_NUM)+1 end ORDERNUM from TB4D_DEV_DEMO_GEN_LIST
    </select>
</mapper>